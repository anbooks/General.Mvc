<!DOCTYPE html>
<html>
<head>
    <meta charset="utf-8" />
    <meta name="viewport" content="width=device-width, initial-scale=1.0" />
    <meta http-equiv="X-UA-Compatible" content="IE=edge,chrome=1" />
    <title>@ViewData["Title"] - 通用管理中心</title>
    <link href="/assets/css/bootstrap.min.css" rel="stylesheet" />
    <link href="/assets/css/font-awesome.min.css" rel="stylesheet" />
    <link href="/assets/css/jquery.gritter.css" rel="stylesheet" />
    <link href="/assets/css/ace-fonts.css" rel="stylesheet" />
    <link href="/assets/css/ace.min.css" rel="stylesheet" />
    <link href="/assets/css/ace-skins.min.css" rel="stylesheet" />
    <link href="/assets/css/ace-rtl.min.css" rel="stylesheet" />
    <link href="/assets/css/ace-part2.min.css" rel="stylesheet" />
    <link href="/assets/css/jquery-ui.custom.min.css" rel="stylesheet" />
    <link href="/assets/css/bootstrap-editable.css" rel="stylesheet" />
    <link href="/assets/css/datepicker.css" rel="stylesheet" />
    <link href="~/css/site.css" rel="stylesheet" />

    <link rel="stylesheet" type="text/css" href="~/assets/css/bootstrap-3.3.4.css">
    <link rel="stylesheet" type="text/css" href="~/assets/css/bootstrap-3.3.4.css">
    <link rel="stylesheet" type="text/css" href="~/assets/font-awesome/4.6.0/css/font-awesome.4.6.0.css">
    <link rel="stylesheet" href="~/assets/css/jquery-ui.min.css">
    <link rel="stylesheet" href="~/assets/css/bootstrap-datepicker3.min.css" />
    <link rel="stylesheet" href="~/assets/css/ui.jqgrid.min.css" />
    <link rel="stylesheet" href="~/assets/css/ace.min.css" class="ace-main-stylesheet" id="main-ace-style" />

    <link rel="stylesheet" href="~/assets/css/ace-skins.min.css" />
    <link rel="stylesheet" href="~/assets/css/ace-rtl.min.css" />

    <script src="~/assets/js/ace-extra.min.js"></script>

    <script src="~/assets/js/jquery.min.js"></script>
</head>
<body class="no-skin">
    <div id="navbar" class="navbar navbar-default navbar-fixed-top">
        <div class="navbar-container" id="navbar-container">
            <button type="button" class="navbar-toggle menu-toggler pull-left" id="menu-toggler">
                <span class="sr-only">Toggle sidebar</span>
                <span class="icon-bar"></span>
                <span class="icon-bar"></span>
                <span class="icon-bar"></span>
            </button>
            <div class="navbar-header pull-left">
                <a href="@Url.RouteUrl("mainIndex")" class="navbar-brand">
                    <small>
                        <i class=""></i>
                        通用管理系统
                    </small>
                </a>
            </div>
            @{ Html.RenderPartial("_LoginPartial");}
        </div>
    </div>
    <div class="main-container" id="main-container">
        @{ Html.RenderPartial("_NavbarPartial");}
        <div class="main-content">
            @RenderBody()
        </div>
    </div>
    <div class="footer">
        <div class="footer-inner">
            <!-- #section:basics/footer -->
            <div class="footer-content">
                <span class="bigger-120">
                    <span class="blue bolder">Ace</span>
                    通用管理系统 © 2017-@DateTime.Now.Year
                </span>
            </div>
            <!-- /section:basics/footer -->
        </div>
    </div>
    <script src="~/assets/js/bootstrap.min.js"></script>
    <script src="~/assets/js/ace.min.js"></script>
    <script src="~/assets/js/ace-elements.min.js"></script>
    <script src="~/lib/jquery-validation/dist/jquery.validate.js"></script>
    <script src="~/lib/jquery-validation-unobtrusive/jquery.validate.unobtrusive.js"></script>
    <script src="~/assets/js/bootbox.min.js"></script>
    <script src="~/js/jquery.modal.js"></script>
    <script src="~/js/jquery.form.js"></script>
    <script src="~/js/jquery.ajax.setup.js"></script>


    <script src="~/assets/js/jquery-ui.custom.min.js"></script>
    <script src="~/assets/js/jquery.ui.touch-punch.min.js"></script>
    <script src="~/assets/js/jquery.gritter.min.js"></script>
    <script src="~/assets/js/bootbox.js"></script>
    <script src="~/assets/js/jquery.easypiechart.min.js"></script>
    <script src="~/assets/js/bootstrap-datepicker.min.js"></script>
    <script src="~/assets/js/jquery.hotkeys.index.min.js"></script>
    <script src="~/assets/js/bootstrap-wysiwyg.min.js"></script>
    <script src="~/assets/js/select2.min.js"></script>
    <script src="~/assets/js/spinbox.min.js"></script>
    <script src="~/assets/js/bootstrap-editable.min.js"></script>
    <script src="~/assets/js/ace-editable.min.js"></script>
    <script src="~/assets/js/jquery.maskedinput.min.js"></script>

    @RenderSection("scripts", required: false)
</body>
</html>
