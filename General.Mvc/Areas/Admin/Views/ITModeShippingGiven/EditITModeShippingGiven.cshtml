@using General.Entities
@model ImportTrans_main_record
@{
    ViewData["Title"] = "编辑运输方式";
}
<div class="breadcrumbs" id="breadcrumbs">
    <ul class="breadcrumb">
        <li>
            <i class="ace-icon fa fa-home home-icon"></i>
            <a href="@Url.RouteUrl("mainIndex")">首页</a>
        </li>
        <li><a href="@ViewBag.ReturnUrl">进口运输</a></li>
        <li class="active">@ViewBag.Title</li>
    </ul>
</div>

<div class="page-content">
    <div class="page-header">
        <h1>
            @ViewData["Title"]
        </h1>
    </div>
    <div class="row">
        <div class="col-sm-12">
            <form asp-route="editITModeShippingGiven" method="post" class="form-horizontal">
                @Html.HiddenFor(m => m.Id)
                <div asp-validation-summary="All"></div>

                <div class="form-group">
                    <label class="col-sm-2 control-label no-padding-right">运输方式</label>
                    <div class="col-sm-3">
                        <select asp-for="ShippingMode" class="form-control" asp-items="ViewBag.ShippingModelist"></select>
                    </div>
                    <span asp-validation-for="ShippingMode" class="inline text-danger"></span>
                </div>



                <div class="col-md-offset-2 col-md-9">
                    <button class="btn btn-info" type="submit">
                        <i class="ace-icon fa fa-check bigger-110"></i>
                        提交
                    </button>
                </div>
            </form>
        </div>
    </div>
</div>