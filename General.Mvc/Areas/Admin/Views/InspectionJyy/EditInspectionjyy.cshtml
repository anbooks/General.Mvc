
@using General.Entities
@model ImportTrans_main_record
@{
    ViewData["Title"] = "质保单";
}
<div class="breadcrumbs" id="breadcrumbs">
    <ul class="breadcrumb">
        <li>
            <i class="ace-icon fa fa-home home-icon"></i>
            <a href="@Url.RouteUrl("mainIndex")">首页</a>
        </li>
        <li><a href="@ViewBag.ReturnUrl">质保单</a></li>
        <li class="active">@ViewBag.Title</li>
    </ul>
</div>

<div class="page-content">
    <div class="page-header">
        <h1>
            @ViewData["Title"]
        </h1>
    </div>
    <div class="row">
        <div class="col-sm-12">
            
            <form asp-route="editInspectionJyyAttachment" method="post" enctype="multipart/form-data" class="form-horizontal">
                @Html.HiddenFor(m => m.Id)
                <div asp-validation-summary="All"></div>
               
   
                        <div class="form-group">
                            <div class="col-sm-2 control-label no-padding-right">质保单附件上传</div>
                            <div class="col-sm-3">
                                <input type="file" name="files" multiple />
                            </div>
                        </div>
                   
                <div class="col-md-offset-2 col-md-9">
                    <button class="btn btn-info" type="submit">
                        <i class="ace-icon fa fa-check bigger-110"></i>
                        提交
                    </button>
                </div>
            </form>
        </div>
    </div>
</div>